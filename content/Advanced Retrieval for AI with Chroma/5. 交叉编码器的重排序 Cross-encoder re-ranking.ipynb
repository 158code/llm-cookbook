      "1.2505131\n",
      "3.7234032\n",
      "3.4397516\n",
      "5.0577946\n",
      "7.2845283\n",
      "6.2345767\n"
     ]
    }
   ],
   "source": [
    "pairs = [[query, doc] for doc in retrieved_documents]\n",
    "scores = cross_encoder.predict(pairs)\n",
    "print(\"Scores:\")\n",
    "for score in scores:\n",
    "    print(score)"
   ]
